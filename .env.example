# =============================================================================
# SC2 Stats Project Environment Configuration
# =============================================================================

# Liquipedia API Configuration
LIQUIPEDIA_USER_AGENT=sc2stats/1.0 (contact: your-email@example.com)
LIQUIPEDIA_API_URL=https://liquipedia.net/starcraft2/api.php

# Rate Limiting and Retry Settings
RATE_LIMIT_DELAY=1.0
MAX_RETRIES=5

# Caching Configuration
CACHE_TTL=3600
CACHE_DIR=cache

# Supabase Database Configuration
SUPABASE_URL=your-supabase-project-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key
SUPABASE_DATABASE_URL=postgresql://postgres:password@your-supabase-project.supabase.co:5432/postgres

# Frontend Environment Variables (Vite requires VITE_ prefix)
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
